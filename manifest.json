{
  "manifest_version": 2,
  "name": "Bookmarks Plus",
  "version": "0.1.0",
  "description": "A bookmarks app.",
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png"
  },
  "homepage_url": "https://github.com/AndersDJohnson/chrome-bookmarks-plus",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "bookmarks"
  ]
}
